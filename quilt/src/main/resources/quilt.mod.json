{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${group}",
        "id": "${mod_id}",
        "version": "${version}",
        "metadata": {
            "mod_name": "${mod_name}",
            "description": "${description}",
            "contributors": {
                "${mod_author}": "Owner"
            },
            "license": "${license}",
            "contact": {
                "homepage": "https://github.com/ItsMaddieNow/Building-Tweaks",
                "issues": "https://github.com/ItsMaddieNow/Building-Tweaks/issues",
                "sources": "https://github.com/ItsMaddieNow/Building-Tweaks"
            },
            "icon": "${mod_id}.png"
        },
        "intermediate_mappings": "net.fabricmc:intermediary",
        "entrypoints": {
            "main": [
                "com.example.examplemod.ExampleMod"
            ]
        },
        "depends": [
            {
                "id": "quilt_loader",
                "versions": ">=${quilt_loader_version}"
            },
            {
                "id": "quilted_fabric_api",
                "versions": "*"
            },
            {
                "id": "minecraft",
                "versions": "${minecraft_version}"
            }
        ]
    },
    "mixin": [
        "${mod_id}.mixins.json",
        "${mod_id}.quilt.mixins.json"
    ]
}
  