{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${group}",
        "id": "${mod_id}",
        "version": "${version}",
        "name": "${mod_name}",
        "description": "${description}",
        "authors": [
            "${mod_author}"
        ],
        "contact": {
            "homepage": "https://github.com/ItsMaddieNow/Building-Tweaks",
            "issues": "https://github.com/ItsMaddieNow/Building-Tweaks/issues",
            "sources": "https://github.com/ItsMaddieNow/Building-Tweaks"
        },
        "license": "${license}",
        "icon": "${mod_id}.png",
        "environment": "*",
        "entrypoints": {
            "main": [
                "com.example.examplemod.ExampleMod"
            ]
        },
        "mixins": [
            "${mod_id}.mixins.json",
            "${mod_id}.quilt.mixins.json"
        ],
        "depends": [
            {
                "id": "quilt_loader",
                "versions": ">=${quilt_loader_version}"
            },
            {
                "id": "quilted_fabric_api",
                "versions": "*"
            },
            {
                "id": "minecraft",
                "versions": "${minecraft_version}"
            }
        ],
        "suggests": {
            "another-mod": "*"
        }
    }
}
  